<% provide(:title, 'Discuss') %>
<h1>My Groups</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <% names = Membership.joins(:group).select(:name).where(user_id: current_user.id) %>
    <ul>
    	<% names.each do |name|%>
    	<li>
    		<%= name['name'] %>
    	</li>
    	<% end %>
    </ul>
  </div>
</div>