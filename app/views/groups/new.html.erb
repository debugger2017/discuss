<% provide(:title, 'Create Group') %>
<h1>Create Group</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@group) do |f| %>
     
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.submit "Submit", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>